{
    "manifest_version": 3,
    "name": "NeoBar",
    "version": "1.1",
    "description": "A sidebar extension to aid with playing Neopets.",
    "permissions": ["notifications"],
    "action": {
        "default_panel": "popup.html",
        "default_icon": "img/icon.gif"
      },
      "background": {
        "service_worker": "background.js"
      }
    }
  